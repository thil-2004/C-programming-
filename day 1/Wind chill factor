#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {   double t,v;
        scanf("%lf%lf",&t,&v);
        double WCF=(35.74+0.6215*t+(0.4275*t-35.75)*pow(v,0.16));
        printf("%.2lf",WCF);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
